    "bugs": {
        "url": "https://github.com/adbahrani/MSGraphDashBoard/issues"
    },
    "devDependencies": {
        "concurrently": "^8.2.0",
        "eslint": "^8.45.0",
        "heroku": "^8.1.9",
        "husky": "^8.0.3",
        "nodemon": "^3.0.1"
    },
    "description": "",
    "dependencies": {
        "@fontsource/roboto": "^5.0.8",
        "dotenv": "^16.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
